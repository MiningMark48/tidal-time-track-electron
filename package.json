{
  "name": "tidal-time-track",
  "version": "0.0.4",
  "description": "An application to track all your program usage.",
  "main": "main.js",
  "build": {
    "appId": "miningmark48.tidal-time-track",
    "artifactName": "tidal-time-track-v${version}.${ext}",
    "mac": {
      "category": "miningmark48.tidal-time-track.utilty"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "publish": "electron-builder --x64 -p always",
    "erebuild": "./node_modules/.bin/electron-rebuild"
  },
  "repository": "git@github.com:MiningMark48/tidal-time-track-electron.git",
  "keywords": [
    "time",
    "tracker"
  ],
  "author": "MiningMark48",
  "license": "Apache 2.0",
  "devDependencies": {
    "asar": "^1.0.0",
    "electron": "^4.2.0",
    "electron-builder": "^20.40.2",
    "electron-rebuild": "^1.8.4"
  },
  "dependencies": {
    "active-win": "^5.0.0",
    "binding": "^3.0.3",
    "electron-chartjs": "^1.0.0",
    "electron-debug": "^2.2.0",
    "electron-json-storage": "^4.1.6",
    "electron-log": "^3.0.5",
    "electron-preferences": "^1.3.0",
    "electron-updater": "^4.0.6",
    "electron-window-state": "^5.0.3",
    "export-from-json": "^1.1.3",
    "jquery": "^3.4.0",
    "keytar": "^4.6.0",
    "react": "^16.8.6",
    "ref": "^1.3.5"
  }
}
